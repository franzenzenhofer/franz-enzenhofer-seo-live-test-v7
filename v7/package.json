{
  "name": "f19n-obtrusive-livetest-v7",
  "version": "0.1.864",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/bump-version.mjs && npm run rules:json && npm run typecheck && npm run lint",
    "clean:dist": "rm -rf dist .vite",
    "build": "npm run prebuild && npm run clean:dist && cross-env GOOGLE_OAUTH_CLIENT_ID=$(node scripts/set-oauth-env.mjs) NODE_OPTIONS=\"--require ./scripts/fix-undici.cjs\" vite build && npm run verify:build && npm test && npm run dev:reload-token && npm run test:e2e:quick && npm run zip:build",
    "build:dev": "cross-env GOOGLE_OAUTH_CLIENT_ID=$(node scripts/set-oauth-env.mjs) EXT_ENV=dev vite build && npm run verify:build && npm run dev:reload-token",
    "fastbuild": "npm run rules:json && npm run typecheck && npm run lint && cross-env GOOGLE_OAUTH_CLIENT_ID=$(node scripts/set-oauth-env.mjs) NODE_OPTIONS=\"--require ./scripts/fix-undici.cjs\" vite build && npm run verify:build && npm run dev:reload-token",
    "dist": "npm run build && tsx scripts/dist.ts",
    "zip:build": "tsx scripts/zip-build.ts",
    "verify:build": "tsx scripts/verify-build-config.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "node scripts/vitest-runner.mjs",
    "test:watch": "vitest --watch --reporter=dot",
    "test:changed": "vitest --changed --watch --reporter=dot --passWithNoTests",
    "test:e2e": "npm run build:dev && npx playwright test",
    "test:e2e:quick": "npx playwright test",
    "test:e2e:dev": "cross-env PW_EXT_HEADLESS=0 npx playwright test --project=chromium --headed",
    "cli": "tsx src/cli/main.ts",
    "ci": "npm run typecheck && npm run lint && npm test && npm run build && npm run test:build",
    "test:build": "vitest run tests/build.html-files.test.ts",
    "check": "npm run typecheck && npm run lint",
    "check:watch": "chokidar \"src/**/*.{ts,tsx,js,jsx}\" -c \"npm run check\"",
    "pack": "rm -f dist.zip && cd dist && zip -r ../dist.zip . >/dev/null",
    "rules:json": "tsx scripts/export-rules.ts",
    "dev:reload-token": "tsx scripts/write-reload-token.ts",
    "chrome:profile": "tsx scripts/open-profile.ts",
    "bump": "node scripts/bump-version.mjs"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0",
    "@playwright/test": "^1.48.2",
    "@types/chrome": "^0.0.268",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.8.28",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0-canary-7118f5dd7-20230705",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "sade": "^1.8.1",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.3"
  }
}
